<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="provider"/>

    <!--<dubbo:registry address="N/A"/>-->

    <dubbo:registry protocol="zookeeper" address="139.199.208.159:2181" client="zkclient" />

    <dubbo:protocol name="dubbo" port="20880"/>

    <bean id="userService" class="com.sso.service.userservice.UserServiceImp"/>

    <dubbo:service interface="org.project.service.UserService" ref="userService" />

</beans>